$('#<%= @post.id %>_likeCount').html("<%= @post.up_votes %> Likes");
$('#<%= @post.id %>_dislikeCount').html("<%= @post.down_votes %> Dislikes");

<% if @vote.errors.messages.any? %>
  $('#<%= @post.id %>_message').html("You can only vote once (๑•̀ㅂ•́)و✧");
<% else %>
  $('#<%= @post.id %>_message').html("Your vote was counted (๑•̀ㅂ•́)و✧");
<% end %>
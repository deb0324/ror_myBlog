<%= render 'shared/title', title: 'New Frame'%>

<div class="col-md-7 well">
  <%= form_for [@gallery, @frame], :html => {:multipart => true} do |f| %>
    <%= render 'shared/form_errors', object: @frame %>

    <div class="form-group ">
      <label>Name</label>
      <p><%= f.text_field :name, :placeholder => "name", class: "form-control" %></p>
    </div>

    <div class="form-group ">
      <label>Image</label>
      <p><%= f.file_field :image %></p>
    </div>

    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>
